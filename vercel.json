{
    "version": 2,
    "builds": [
      {
        "src": "index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/login",
        "methods": ["GET"],
        "dest": "/index.js"
      },
      {
        "src": "/login/(.*)",
        "methods": ["PUT"],
        "dest": "/index.js"
      }
    ],
    "env": {
      "tokenT": "@tokenT",    
      "gst1": "@gst1",      
      "gst2": "@gst2"        
    }
  }
  